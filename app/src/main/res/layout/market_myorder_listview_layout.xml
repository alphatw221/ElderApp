<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:background="#FFFFFF">


    <ImageView
        android:id="@+id/_market_myorder_listview_image"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.3"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/_market_myorder_listview_recieve"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FFFFFF"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.3"
        app:srcCompat="@drawable/receive" />

    <TextView
        android:id="@+id/_market_myorder_listview_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textColor="@android:color/black"
        app:layout_constraintStart_toEndOf="@+id/_market_myorder_listview_image"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/_market_myorder_listview_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        android:textColor="#FF000000"
        app:layout_constraintStart_toEndOf="@+id/_market_myorder_listview_image"
        app:layout_constraintTop_toBottomOf="@+id/_market_myorder_listview_name" />

    <TextView
        android:id="@+id/_market_myorder_listview_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        android:textColor="#FF000000"
        app:layout_constraintStart_toEndOf="@+id/_market_myorder_listview_image"
        app:layout_constraintTop_toBottomOf="@+id/_market_myorder_listview_location" />

    <TextView
        android:id="@+id/_market_myorder_listview_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        android:textColor="#FF000000"
        app:layout_constraintStart_toEndOf="@+id/_market_myorder_listview_image"
        app:layout_constraintTop_toBottomOf="@+id/_market_myorder_listview_address" />
</androidx.constraintlayout.widget.ConstraintLayout>